{
  "name": "@thesis/scripts",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "quality-dashboard": "./src/quality-dashboard.mjs"
  },
  "scripts": {
    "quality-dashboard": "node src/quality-dashboard.mjs",
    "generate-thesis": "node src/generate-thesis-docx.mjs",
    "detailed-report": "node src/detailed-metrics-report.mjs --projects ../../../input/projects.json --outputDir ../../../reports",
    "temporal-report": "node src/temporal-metrics-report.mjs --projects ../../../input/projects.json --outputDir ../../../reports",
    "test-real": "node src/test-real-metrics.mjs",
    "test-fixed": "node src/test-fixed-metrics.mjs",
    "test-improved": "node src/test-improved-metrics.mjs",
    "verify-projects": "node src/verify-projects.mjs",
    "test-pr-fix": "node src/test-pr-iteration-fix.mjs",
    "debug-api": "node src/debug-github-api.mjs",
    "verify-replacements": "node src/verify-replacements.mjs",
    "debug-pr": "node src/debug-pr-iteration.mjs"
  },
  "nx": {
    "name": "scripts"
  },
  "dependencies": {
    "@thesis/metrics": "*",
    "@thesis/metrics-collector": "*",
    "docx": "^8.5.0"
  }
}
